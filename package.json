{
  "name": "oh-my-ag",
  "version": "1.13.1",
  "description": "Professional agent skills for Google Antigravity IDE - PM, Frontend, Backend, Mobile, QA, and Debug specialists that work together seamlessly",
  "type": "module",
  "bin": {
    "oh-my-ag": "./bin/cli.js"
  },
  "files": [
    "bin",
    ".agent/skills"
  ],
  "keywords": [
    "antigravity",
    "skills",
    "agent-skills",
    "multi-agent",
    "pm",
    "frontend",
    "backend",
    "mobile",
    "qa",
    "debug",
    "bug-fixing",
    "gemini"
  ],
  "author": "our.first.fluke <our.first.fluke@gmail.com>",
  "contributors": [
    "gracefullight <gracefullight.dev@gmail.com>",
    "gahyun-git <go4it.gh@gmail.com>"
  ],
  "license": "MIT",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/first-fluke"
    },
    {
      "type": "buymeacoffee",
      "url": "https://buymeacoffee.com/firstfluke"
    }
  ],
  "scripts": {
    "build": "bun build src/cli.ts --outfile bin/cli.js --target node --minify",
    "dev": "bun run src/cli.ts",
    "lint": "biome check --write --unsafe .",
    "test": "vitest run",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "chokidar": "^4.0.0",
    "commander": "^12.0.0",
    "p-map": "^7.0.4",
    "picocolors": "^1.1.1",
    "ws": "^8.18.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/bun": "^1.3.8",
    "@types/ws": "^8.18.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/first-fluke/oh-my-ag"
  },
  "antigravity": {
    "skillsPath": ".agent/skills",
    "skills": [
      "workflow-guide",
      "pm-agent",
      "frontend-agent",
      "backend-agent",
      "mobile-agent",
      "qa-agent",
      "debug-agent",
      "orchestrator"
    ]
  }
}
